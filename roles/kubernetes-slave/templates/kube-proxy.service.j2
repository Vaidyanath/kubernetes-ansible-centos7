[Unit]
Description=Kubernetes Proxy
After=network.target

[Service]
EnvironmentFile=-{{ kube_config_dir }}/{{ kube_proxy_config_file }}
ExecStart=/usr/local/bin/kube-proxy \
					${KUBE_LOGTOSTDERR} \
                    ${KUBE_LOG_LEVEL} \
                    ${NODE_HOSTNAME} \
                    ${KUBE_MASTER}
Restart=on-failure
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target
